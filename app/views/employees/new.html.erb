<h1>Employee Info</h1>
<%= form_for(@employee) do |f| %>
    <%= f.label(:full_name) %>
    <%= f.text_field(:full_name) %>
    <br />
    <%= f.label(:dob) %>
    <%= f.text_field(:dob) %>
    <br />
    <%= f.label(:job_title) %>
    <%= f.text_field(:job_title) %>
    <br />
    <%= label_tag(:organization_name) %>
    <%#= <%= text_field_tag(:organization_name) %>
    <%= collection_select(:name, 0, Organization.all, :id, :name) %>
    <br />
    <%= f.submit %>
<% end %>